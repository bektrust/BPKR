				ТабличныйДокумент.Вывести(ОбластьДеталиПустаяСтрока);
			КонецЦикла;
		КонецЕсли;
		
		ДанныеПечати.Вставить("Количество", ИтогоКоличество);	
		ДанныеПечати.Вставить("Сумма", ИтогоСумма);
		
