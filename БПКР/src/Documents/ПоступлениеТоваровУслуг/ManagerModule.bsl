	Запрос.УстановитьПараметр("ВалютаРасчетовГНС", ДоговорКонтрагентаГНС.ВалютаРасчетов);

	Запрос.УстановитьПараметр("СчетРасчетовГНС", СчетаУчета.СчетРасчетовПоставщика);
	Запрос.УстановитьПараметр("СчетРасчетовТоварыЕАЭС", СчетРасчетовТоварыЕАЭС);
	Запрос.УстановитьПараметр("СчетРасчетовУслугиЕАЭС", СчетРасчетовУслугиЕАЭС);
	Запрос.УстановитьПараметр("СчетРасчетовУслугиИмпорт", СчетРасчетовУслугиИмпорт);	

	Запрос.Выполнить();    		
		
	СформироватьТаблицаХозрасчетный(ДокументСсылка, СтруктураДополнительныеСвойства);	
	СформироватьТаблицаПоступлениеТоваров(ДокументСсылка, СтруктураДополнительныеСвойства);
	СформироватьТаблицаПараметрыУчетаОС(ДокументСсылка, СтруктураДополнительныеСвойства); 
