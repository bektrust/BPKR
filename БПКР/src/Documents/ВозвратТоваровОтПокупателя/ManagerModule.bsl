						ДанныеПечати.СтоимостьПоЛисту 			= 0;
						ДанныеПечати.СуммаНДСПоЛисту 			= 0;
						ДанныеПечати.СуммаНСППоЛисту 			= 0;
						ДанныеПечати.ВсегоПоЛисту 				= 0;
