	ПоступлениеТоваровОбороты.Организация.*,
	ПоступлениеТоваровОбороты.Контрагент.*,
	ПоступлениеТоваровОбороты.Договор.*,
	ПоступлениеТоваровОбороты.Номенклатура.*,
	ПоступлениеТоваровОбороты.ЗачетНДС.*,
	ПоступлениеТоваровОбороты.Регистратор.*,
	ПоступлениеТоваровОбороты.Склад.*}</query>
