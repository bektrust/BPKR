		<field xsi:type="DataSetFieldField">
			<dataPath>АкцизОборот</dataPath>
			<field>АкцизОборот</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ОбщаяЦенаОборот</dataPath>
			<field>ОбщаяЦенаОборот</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВалютаСом</dataPath>
			<field>ВалютаСом</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Валюта сом</v8:content>
				</v8:item>
			</title>
