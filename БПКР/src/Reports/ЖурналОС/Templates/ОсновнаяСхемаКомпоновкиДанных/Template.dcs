ИНДЕКСИРОВАТЬ ПО
	Организация,
	ОсновноеСредство
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ЕСТЬNULL(ВТ_МестонахождениеОС.Подразделение, ЗНАЧЕНИЕ(Справочник.ПодразделенияОрганизаций.ПустаяСсылка)) КАК Подразделение,
	ВТ_ОС.ОсновноеСредство.Родитель КАК Группа,
	ВТ_ОС.ОсновноеСредство КАК ОсновноеСредство,
	ВТ_СчетаУчетаОС.СчетУчета КАК СчетУчета,
	ЕСТЬNULL(ВТ_ПервоначальныеСведенияОС.ИнвентарныйНомер, "") КАК ИнвентарныйНомер,
	ЕСТЬNULL(ВТ_ПервоначальныеСведенияОС.ПервоначальнаяСтоимость, 0) КАК ПервоначальнаяСтоимость,
	ЕСТЬNULL(ВТ_Стоимость.Приход, 0) КАК Приход,
	ЕСТЬNULL(ВТ_Стоимость.Расход, 0) КАК Расход,
	ЕСТЬNULL(ВТ_Амортизация.ИзносЗаПериод, 0) КАК ИзносЗаПериод,
	ЕСТЬNULL(ВТ_Амортизация.НакопленныйИзнос, 0) КАК НакопленныйИзнос,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВТ_Стоимость.ОстаточнаяСтоимость, 0) &lt;&gt; 0
			ТОГДА ЕСТЬNULL(ВТ_Стоимость.ОстаточнаяСтоимость, 0) - ЕСТЬNULL(ВТ_Амортизация.НакопленныйИзнос, 0)
		ИНАЧЕ 0
	КОНЕЦ КАК ОстаточнаяСтоимость,
	ДАТАВРЕМЯ(1, 1, 1) КАК ДатаКапитализации,
	0 КАК СуммаКапитализации,
	"Нет" КАК СрокСлужбыПересмотрен,
	ЕСТЬNULL(ВТ_ПервоначальныйСрокСлужбы.СрокСлужбы, 0) КАК СрокСлужбы,
	ВЫБОР
		КОГДА ВТ_ДатыПриобретенияОС.ДатаПриобретения ЕСТЬ NULL
				ИЛИ ВТ_ДатыПриобретенияОС.ДатаПриобретения = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА ЕСТЬNULL(ВТ_ДатыПринятияКУчетуОС.ДатаПринятияКУчету, ДАТАВРЕМЯ(1, 1, 1))
		ИНАЧЕ ВТ_ДатыПриобретенияОС.ДатаПриобретения
	КОНЕЦ КАК ДатаПриобретения,
	ЕСТЬNULL(ВТ_ДатыПринятияКУчетуОС.ДатаПринятияКУчету, ДАТАВРЕМЯ(1, 1, 1)) КАК ДатаПринятияКУчету,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВТ_ПараметрыАмортизацииОС.СрокСлужбы, 0) - РАЗНОСТЬДАТ(ЕСТЬNULL(ВТ_ДатыПринятияКУчетуОС.ДатаПринятияКУчету, ДАТАВРЕМЯ(1, 1, 1)), &amp;КонецПериода, МЕСЯЦ) &lt; 0
			ТОГДА 0
		ИНАЧЕ ЕСТЬNULL(ВТ_ПараметрыАмортизацииОС.СрокСлужбы, 0) - РАЗНОСТЬДАТ(ЕСТЬNULL(ВТ_ДатыПринятияКУчетуОС.ДатаПринятияКУчету, ДАТАВРЕМЯ(1, 1, 1)), &amp;КонецПериода, МЕСЯЦ)
	КОНЕЦ КАК ОставшийсяСрокСлужбы,
	ЕСТЬNULL(ВТ_ПараметрыАмортизацииОС.СрокСлужбы, 0) КАК СрокСлужбыФакт,
	ВЫБОР
		КОГДА ДОБАВИТЬКДАТЕ(ЕСТЬNULL(ВТ_ДатыПринятияКУчетуОС.ДатаПринятияКУчету, ДАТАВРЕМЯ(1, 1, 1)), МЕСЯЦ, РАЗНОСТЬДАТ(ЕСТЬNULL(ВТ_ДатыПринятияКУчетуОС.ДатаПринятияКУчету, ДАТАВРЕМЯ(1, 1, 1)), &amp;КонецПериода, МЕСЯЦ)) &gt; &amp;КонецПериода
			ТОГДА РАЗНОСТЬДАТ(ЕСТЬNULL(ВТ_ДатыПринятияКУчетуОС.ДатаПринятияКУчету, ДАТАВРЕМЯ(1, 1, 1)), &amp;КонецПериода, МЕСЯЦ) - 1
		ИНАЧЕ РАЗНОСТЬДАТ(ЕСТЬNULL(ВТ_ДатыПринятияКУчетуОС.ДатаПринятияКУчету, ДАТАВРЕМЯ(1, 1, 1)), &amp;КонецПериода, МЕСЯЦ)
	КОНЕЦ КАК СрокСлужбыИстекший,
	ВТ_МестонахождениеОС.МОЛ КАК МОЛ,
	ВТ_ОС.Организация КАК Организация,
	ВЫБОР
		КОГДА ВТ_ОС.Состояние = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийОС.СнятоСУчета)
			ТОГДА ВТ_ОС.ДатаСостояния
		ИНАЧЕ ДАТАВРЕМЯ(1, 1, 1)
	КОНЕЦ КАК ДатаВыбытия,
	ВТ_ОС.ДатаВыпуска КАК ДатаВыпуска,
	ВТ_Амортизация.ИзносЗаПериод КАК АмортизацияЗаМесяц,
	ВТ_ПервоначальныеСведенияОС.ЛиквидационнаяСтоимость КАК ЛиквидационнаяСтоимость
ПОМЕСТИТЬ ВТ_ИтоговыеДанные
{ВЫБРАТЬ
	Подразделение.*,
	Группа.*,
	ОсновноеСредство.*,
	СчетУчета.*,
	ИнвентарныйНомер,
	ПервоначальнаяСтоимость,
	Приход,
	Расход,
	ИзносЗаПериод,
	НакопленныйИзнос,
	ОстаточнаяСтоимость,
	ДатаКапитализации,
	СуммаКапитализации,
	СрокСлужбыПересмотрен,
	СрокСлужбы,
	ДатаПриобретения,
	ДатаПринятияКУчету,
	ОставшийсяСрокСлужбы,
	СрокСлужбыФакт,
	СрокСлужбыИстекший,
	МОЛ.*,
	Организация.*,
	ДатаВыбытия,
	ДатаВыпуска,
	АмортизацияЗаМесяц,
	ЛиквидационнаяСтоимость}
ИЗ
	ВТ_ОС КАК ВТ_ОС
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ПервоначальныеСведенияОС КАК ВТ_ПервоначальныеСведенияОС
		ПО ВТ_ОС.ОсновноеСредство = ВТ_ПервоначальныеСведенияОС.ОсновноеСредство
			И ВТ_ОС.Организация = ВТ_ПервоначальныеСведенияОС.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Стоимость КАК ВТ_Стоимость
		ПО ВТ_ОС.ОсновноеСредство = ВТ_Стоимость.Субконто1
			И ВТ_ОС.Организация = ВТ_Стоимость.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Амортизация КАК ВТ_Амортизация
		ПО ВТ_ОС.ОсновноеСредство = ВТ_Амортизация.Субконто1
			И ВТ_ОС.Организация = ВТ_Амортизация.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СчетаУчетаОС КАК ВТ_СчетаУчетаОС
		ПО ВТ_ОС.ОсновноеСредство = ВТ_СчетаУчетаОС.ОсновноеСредство
			И ВТ_ОС.Организация = ВТ_СчетаУчетаОС.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ПараметрыАмортизацииОС КАК ВТ_ПараметрыАмортизацииОС
		ПО ВТ_ОС.ОсновноеСредство = ВТ_ПараметрыАмортизацииОС.ОсновноеСредство
			И ВТ_ОС.Организация = ВТ_ПараметрыАмортизацииОС.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ДатыПриобретенияОС КАК ВТ_ДатыПриобретенияОС
		ПО ВТ_ОС.ОсновноеСредство = ВТ_ДатыПриобретенияОС.ОсновноеСредство
			И ВТ_ОС.Организация = ВТ_ДатыПриобретенияОС.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ДатыПринятияКУчетуОС КАК ВТ_ДатыПринятияКУчетуОС
		ПО ВТ_ОС.ОсновноеСредство = ВТ_ДатыПринятияКУчетуОС.ОсновноеСредство
			И ВТ_ОС.Организация = ВТ_ДатыПринятияКУчетуОС.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_МестонахождениеОС КАК ВТ_МестонахождениеОС
		ПО ВТ_ОС.ОсновноеСредство = ВТ_МестонахождениеОС.ОсновноеСредство
			И ВТ_ОС.Организация = ВТ_МестонахождениеОС.Организация
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_ПервоначальныйСрокСлужбы КАК ВТ_ПервоначальныйСрокСлужбы
		ПО ВТ_ОС.ОсновноеСредство = ВТ_ПервоначальныйСрокСлужбы.ОсновноеСредство
			И ВТ_ОС.Организация = ВТ_ПервоначальныйСрокСлужбы.Организация
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВТ_ИтоговыеДанные.Подразделение КАК Подразделение,
	ВТ_ИтоговыеДанные.Группа КАК Группа,
	ВТ_ИтоговыеДанные.ОсновноеСредство КАК ОсновноеСредство,
	ВТ_ИтоговыеДанные.СчетУчета КАК СчетУчета,
	ВТ_ИтоговыеДанные.ИнвентарныйНомер КАК ИнвентарныйНомер,
	ВЫРАЗИТЬ(ВТ_ИтоговыеДанные.ПервоначальнаяСтоимость КАК ЧИСЛО(15, 2)) КАК ПервоначальнаяСтоимость,
	ВТ_ИтоговыеДанные.Приход КАК Приход,
	ВТ_ИтоговыеДанные.Расход КАК Расход,
	ВТ_ИтоговыеДанные.ИзносЗаПериод КАК ИзносЗаПериод,
	ВЫРАЗИТЬ(ВТ_ИтоговыеДанные.НакопленныйИзнос КАК ЧИСЛО(15, 2)) КАК НакопленныйИзнос,
	(ВЫРАЗИТЬ(ВТ_ИтоговыеДанные.ПервоначальнаяСтоимость КАК ЧИСЛО(15, 2))) - (ВЫРАЗИТЬ(ВТ_ИтоговыеДанные.НакопленныйИзнос КАК ЧИСЛО(15, 2))) - (ВЫРАЗИТЬ(ВТ_ИтоговыеДанные.ЛиквидационнаяСтоимость КАК ЧИСЛО(15, 2))) КАК ОстаточнаяСтоимость,
	(ВЫРАЗИТЬ(ВТ_ИтоговыеДанные.ПервоначальнаяСтоимость КАК ЧИСЛО(15, 2))) - (ВЫРАЗИТЬ(ВТ_ИтоговыеДанные.НакопленныйИзнос КАК ЧИСЛО(15, 2))) КАК БалансоваяСтоимость,
	ВТ_ИтоговыеДанные.ДатаКапитализации КАК ДатаКапитализации,
	ВТ_ИтоговыеДанные.СуммаКапитализации КАК СуммаКапитализации,
	ВТ_ИтоговыеДанные.СрокСлужбыПересмотрен КАК СрокСлужбыПересмотрен,
	ВТ_ИтоговыеДанные.СрокСлужбы КАК СрокСлужбы,
	ВТ_ИтоговыеДанные.ДатаПриобретения КАК ДатаПриобретения,
	ВТ_ИтоговыеДанные.ДатаПринятияКУчету КАК ДатаПринятияКУчету,
	ВТ_ИтоговыеДанные.МОЛ КАК МОЛ,
	ВТ_ИтоговыеДанные.Организация КАК Организация,
	ВТ_ИтоговыеДанные.ДатаВыбытия КАК ДатаВыбытия,
	ВТ_ИтоговыеДанные.ДатаВыпуска КАК ДатаВыпуска,
	ВЫРАЗИТЬ(ВТ_ИтоговыеДанные.ЛиквидационнаяСтоимость КАК ЧИСЛО(15, 2)) КАК ЛиквидационнаяСтоимость,
	ВТ_ИтоговыеДанные.АмортизацияЗаМесяц КАК АмортизацияЗаМесяц
{ВЫБРАТЬ
	Подразделение.*,
	Группа.*,
	ОсновноеСредство.*,
	СчетУчета.*,
	ИнвентарныйНомер,
	ПервоначальнаяСтоимость,
	Приход,
	Расход,
	ИзносЗаПериод,
	НакопленныйИзнос,
	ОстаточнаяСтоимость,
	БалансоваяСтоимость,
	ДатаКапитализации,
	СуммаКапитализации,
	СрокСлужбыПересмотрен,
	СрокСлужбы,
	ДатаПриобретения,
	ДатаПринятияКУчету,
	МОЛ.*,
	Организация.*,
	ДатаВыбытия,
	ДатаВыпуска,
	ЛиквидационнаяСтоимость,
	АмортизацияЗаМесяц}
ИЗ
	ВТ_ИтоговыеДанные КАК ВТ_ИтоговыеДанные
{ГДЕ
	ВТ_ИтоговыеДанные.Подразделение.*,
	ВТ_ИтоговыеДанные.ОсновноеСредство.*}</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
	<totalField>
		<dataPath>ИзносЗаПериод</dataPath>
		<expression>Сумма(ИзносЗаПериод)</expression>
	</totalField>
