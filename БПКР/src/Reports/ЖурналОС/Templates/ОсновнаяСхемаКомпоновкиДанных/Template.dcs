		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	СостоянияОССрезПоследних.Организация КАК Организация,
	СостоянияОССрезПоследних.ОсновноеСредство КАК ОсновноеСредство,
	СостоянияОССрезПоследних.Состояние КАК Состояние,
	СостоянияОССрезПоследних.Период КАК ДатаСостояния,
	СостоянияОССрезПоследних.ОсновноеСредство.ДатаВыпуска КАК ДатаВыпуска
ПОМЕСТИТЬ ВТ_ОС
{ВЫБРАТЬ
	Организация.*,
	ОсновноеСредство.*,
	Состояние.*,
	ДатаСостояния,
	ДатаВыпуска}
ИЗ
	РегистрСведений.СостоянияОС.СрезПоследних({(&amp;КонецПериода)}, ) КАК СостоянияОССрезПоследних
ГДЕ
	(СостоянияОССрезПоследних.Состояние = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийОС.ПринятоКУчету)
			ИЛИ СостоянияОССрезПоследних.Состояние = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийОС.Поступило))
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ПараметрыУчетаОССрезПоследних.ОсновноеСредство КАК ОсновноеСредство,
	ПараметрыУчетаОССрезПоследних.СчетУчета КАК СчетУчета,
	ПараметрыУчетаОССрезПоследних.СчетУчета.ПарныйСчет КАК СчетУчетаПарныйСчет,
	ПараметрыУчетаОССрезПоследних.Организация КАК Организация
ПОМЕСТИТЬ ВТ_СчетаУчетаОС
{ВЫБРАТЬ
	ОсновноеСредство.*,
	СчетУчета.*,
	СчетУчетаПарныйСчет.*,
	Организация.*}
ИЗ
	РегистрСведений.ПараметрыУчетаОС.СрезПоследних(
			{(&amp;КонецПериода)},
			ОсновноеСредство В
				(ВЫБРАТЬ
					ВТ_ОС.ОсновноеСредство
				ИЗ
					ВТ_ОС КАК ВТ_ОС)) КАК ПараметрыУчетаОССрезПоследних

ИНДЕКСИРОВАТЬ ПО
	Организация,
	ОсновноеСредство
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ТиповойОстаткиИОбороты.Субконто1 КАК Субконто1,
	ТиповойОстаткиИОбороты.СуммаКонечныйОстатокДт КАК ОстаточнаяСтоимость,
	ТиповойОстаткиИОбороты.СуммаОборотДт КАК Приход,
	ТиповойОстаткиИОбороты.СуммаОборотКт КАК Расход,
	ТиповойОстаткиИОбороты.Организация КАК Организация
ПОМЕСТИТЬ ВТ_Стоимость
{ВЫБРАТЬ
	Субконто1.*,
	ОстаточнаяСтоимость,
	Приход,
	Расход,
	Организация.*}
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ОстаткиИОбороты(
			{(&amp;НачалоПериода)},
			{(&amp;КонецПериода)},
			,
			,
			Счет В
				(ВЫБРАТЬ РАЗЛИЧНЫЕ
					ВТ_СчетаУчетаОС.СчетУчета
				ИЗ
					ВТ_СчетаУчетаОС КАК ВТ_СчетаУчетаОС),
			ЗНАЧЕНИЕ(ПланВидовХарактеристик.ВидыСубконтоХозрасчетные.ОсновныеСредства),
			Субконто1 В
				(ВЫБРАТЬ
					ВТ_ОС.ОсновноеСредство
				ИЗ
					ВТ_ОС КАК ВТ_ОС)) КАК ТиповойОстаткиИОбороты

ИНДЕКСИРОВАТЬ ПО
	Организация,
	Субконто1
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ТиповойОстаткиИОбороты.Субконто1 КАК Субконто1,
	ТиповойОстаткиИОбороты.СуммаНачальныйОстатокКт + ТиповойОстаткиИОбороты.СуммаОборотКт КАК НакопленныйИзнос,
	ТиповойОстаткиИОбороты.СуммаОборотКт КАК ИзносЗаПериод,
	ТиповойОстаткиИОбороты.Организация КАК Организация
ПОМЕСТИТЬ ВТ_Амортизация
{ВЫБРАТЬ
	Субконто1.*,
	НакопленныйИзнос,
	ИзносЗаПериод,
	Организация.*}
ИЗ
	РегистрБухгалтерии.Хозрасчетный.ОстаткиИОбороты(
			{(&amp;НачалоПериода)},
			{(&amp;КонецПериода)},
			,
			,
			Счет В
				(ВЫБРАТЬ РАЗЛИЧНЫЕ
					ВТ_СчетаУчетаОС.СчетУчета.ПарныйСчет
				ИЗ
					ВТ_СчетаУчетаОС КАК ВТ_СчетаУчетаОС),
			ЗНАЧЕНИЕ(ПланВидовХарактеристик.ВидыСубконтоХозрасчетные.ОсновныеСредства),
			Субконто1 В
				(ВЫБРАТЬ
					ВТ_ОС.ОсновноеСредство
				ИЗ
					ВТ_ОС КАК ВТ_ОС)) КАК ТиповойОстаткиИОбороты

ИНДЕКСИРОВАТЬ ПО
	Организация,
	Субконто1
;

////////////////////////////////////////////////////////////////////////////////
