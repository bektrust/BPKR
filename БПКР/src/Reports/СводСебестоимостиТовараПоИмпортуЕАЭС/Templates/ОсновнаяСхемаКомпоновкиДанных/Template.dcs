		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ТаблицаТовары.Ссылка.Организация КАК Организация,
	ТаблицаТовары.Ссылка КАК ДокументПоступления,
	ТаблицаТовары.Номенклатура КАК Номенклатура,
	ТаблицаТовары.Количество КАК Количество,
	ТаблицаТовары.Цена КАК ЦенаВВалюте,
	ТаблицаТовары.Цена * ТаблицаТовары.Ссылка.Курс / ТаблицаТовары.Ссылка.Кратность КАК ЦенаВСомах,
	ТаблицаТовары.Ссылка.ДоговорКонтрагента.ВалютаРасчетов КАК Валюта,
	//ТаблицаТовары.Сумма - ТаблицаТовары.СуммаАкциза КАК СуммаВСомах,
	ТаблицаТовары.Сумма КАК СуммаВСомах,
	ТаблицаТовары.Всего КАК СуммаДляСоединения,
	ТаблицаТовары.СуммаНДС КАК НДС,
	ТаблицаТовары.СуммаАкциза КАК Акциз,
	ТаблицаТовары.Всего КАК Всего,
	ТаблицаТовары.СчетУчета КАК СчетУчета,
	ВЫБОР
		КОГДА ТаблицаТовары.ЗачетНДС = ЗНАЧЕНИЕ(Перечисление.ВидыЗачетаНДС.Себестоимость)
				ИЛИ &amp;НеПлательщикНДС
			ТОГДА ТаблицаТовары.СуммаНДС
		ИНАЧЕ 0
	КОНЕЦ КАК ОбщаяСуммаНДС
ПОМЕСТИТЬ ВременнаяТаблицаДанныеПоступлений
ИЗ
	Документ.ПоступлениеТоваровУслуг.Товары КАК ТаблицаТовары
ГДЕ
	ТаблицаТовары.Ссылка.Проведен
	И ТаблицаТовары.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ТаблицаТовары.Ссылка.Контрагент.ПризнакСтраны = ЗНАЧЕНИЕ(Перечисление.ПризнакиСтраны.ЕАЭС)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ТаблицаУслуги.Ссылка.Организация,
	ТаблицаУслуги.Ссылка,
	ТаблицаУслуги.Номенклатура,
	ТаблицаУслуги.Количество,
	ТаблицаУслуги.Цена,
	ТаблицаУслуги.Цена * ТаблицаУслуги.Ссылка.Курс / ТаблицаУслуги.Ссылка.Кратность,
	ТаблицаУслуги.Ссылка.ДоговорКонтрагента.ВалютаРасчетов,
	ТаблицаУслуги.Сумма - ТаблицаУслуги.СуммаАкциза,
	ТаблицаУслуги.Всего,
	ТаблицаУслуги.СуммаНДС,
	ТаблицаУслуги.СуммаАкциза,
	0,
	НЕОПРЕДЕЛЕНО,
	ВЫБОР
		КОГДА ТаблицаУслуги.ЗачетНДС = ЗНАЧЕНИЕ(Перечисление.ВидыЗачетаНДС.Себестоимость)
				ИЛИ &amp;НеПлательщикНДС
			ТОГДА ТаблицаУслуги.СуммаНДС
		ИНАЧЕ 0
	КОНЕЦ
ИЗ
	Документ.ПоступлениеТоваровУслуг.Услуги КАК ТаблицаУслуги
ГДЕ
	ТаблицаУслуги.Ссылка.Проведен
	И ТаблицаУслуги.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ТаблицаУслуги.Ссылка.Контрагент.ПризнакСтраны = ЗНАЧЕНИЕ(Перечисление.ПризнакиСтраны.ЕАЭС)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ТаблицаОС.Ссылка.Организация,
	ТаблицаОС.Ссылка,
	ТаблицаОС.ОсновноеСредство,
	1,
	ТаблицаОС.Сумма / ТаблицаОС.Ссылка.Курс * ТаблицаОС.Ссылка.Кратность,
	ТаблицаОС.Сумма,
	ТаблицаОС.Ссылка.ДоговорКонтрагента.ВалютаРасчетов,
	ТаблицаОС.Сумма - ТаблицаОС.СуммаАкциза,
	ТаблицаОС.Всего,
	ТаблицаОС.СуммаНДС,
	ТаблицаОС.СуммаАкциза,
	ТаблицаОС.Всего,
	ТаблицаОС.СчетУчета,
	ВЫБОР
		КОГДА ТаблицаОС.ЗачетНДС = ЗНАЧЕНИЕ(Перечисление.ВидыЗачетаНДС.Себестоимость)
				ИЛИ &amp;НеПлательщикНДС
			ТОГДА ТаблицаОС.СуммаНДС
		ИНАЧЕ 0
	КОНЕЦ
ИЗ
	Документ.ПоступлениеТоваровУслуг.ОС КАК ТаблицаОС
ГДЕ
	ТаблицаОС.Ссылка.Проведен
	И ТаблицаОС.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ТаблицаОС.Ссылка.Контрагент.ПризнакСтраны = ЗНАЧЕНИЕ(Перечисление.ПризнакиСтраны.ЕАЭС)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДополнительныеРасходыТовары.Ссылка.Организация КАК Организация,
	ДополнительныеРасходыТовары.Ссылка КАК ДокументДопрасхода,
	ДополнительныеРасходыТовары.ДокументПоступления КАК ДокументПоступления,
	ДополнительныеРасходыТовары.Номенклатура КАК Номенклатура,
	ДополнительныеРасходыТовары.Количество КАК Количество,
	ДополнительныеРасходыТовары.Цена КАК Цена,
	ДополнительныеРасходыТовары.Сумма КАК Сумма,
	ВЫБОР
		КОГДА ДополнительныеРасходыТовары.ЗачетНДС = ЗНАЧЕНИЕ(Перечисление.ВидыЗачетаНДС.Себестоимость)
			ТОГДА (ДополнительныеРасходыТовары.СуммаРасходов + ДополнительныеРасходыТовары.СуммаРасходовНДС + ДополнительныеРасходыТовары.СуммаРасходовНСП) * ДополнительныеРасходыТовары.Ссылка.Курс / ДополнительныеРасходыТовары.Ссылка.Кратность
		ИНАЧЕ (ДополнительныеРасходыТовары.СуммаРасходов + ДополнительныеРасходыТовары.СуммаРасходовНСП) * ДополнительныеРасходыТовары.Ссылка.Курс / ДополнительныеРасходыТовары.Ссылка.Кратность
	КОНЕЦ КАК ДопРасходы,
	ДополнительныеРасходыТовары.СчетУчета КАК СчетУчета
ПОМЕСТИТЬ ВременнаяТаблицаДопрасходы
ИЗ
	Документ.ДополнительныеРасходы.Товары КАК ДополнительныеРасходыТовары
ГДЕ
	ДополнительныеРасходыТовары.Ссылка.Проведен
	И ДополнительныеРасходыТовары.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ДополнительныеРасходыОС.Ссылка.Организация,
	ДополнительныеРасходыОС.Ссылка,
	ДополнительныеРасходыОС.ДокументПоступления,
	ДополнительныеРасходыОС.ОсновноеСредство,
	ДополнительныеРасходыОС.Количество,
	ДополнительныеРасходыОС.Сумма,
	ДополнительныеРасходыОС.Сумма,
	ВЫБОР
		КОГДА ДополнительныеРасходыОС.ЗачетНДС = ЗНАЧЕНИЕ(Перечисление.ВидыЗачетаНДС.Себестоимость)
			ТОГДА (ДополнительныеРасходыОС.СуммаРасходов + ДополнительныеРасходыОС.СуммаРасходовНДС + ДополнительныеРасходыОС.СуммаРасходовНСП) * ДополнительныеРасходыОС.Ссылка.Курс / ДополнительныеРасходыОС.Ссылка.Кратность
		ИНАЧЕ (ДополнительныеРасходыОС.СуммаРасходов + ДополнительныеРасходыОС.СуммаРасходовНСП) * ДополнительныеРасходыОС.Ссылка.Курс / ДополнительныеРасходыОС.Ссылка.Кратность
	КОНЕЦ,
	ДополнительныеРасходыОС.СчетУчета
ИЗ
	Документ.ДополнительныеРасходы.ОС КАК ДополнительныеРасходыОС
ГДЕ
	ДополнительныеРасходыОС.Ссылка.Проведен
	И ДополнительныеРасходыОС.Ссылка.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВременнаяТаблицаДопрасходы.Организация КАК Организация,
