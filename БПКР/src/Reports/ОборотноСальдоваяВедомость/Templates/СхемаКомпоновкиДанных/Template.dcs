				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>ru</v8:lang>
						<v8:content>Это развернутое сальдо</v8:content>
					</v8:item>
				</title>
			</field>
			<dataSource>ИсточникДанных1</dataSource>
			<query>//Начало ЗапросПоСубконто РазвернутоеСальдо   
ВЫБРАТЬ
	ВложенныйЗапрос.Счет,
	//Null КАК ВложенныйЗапрос.Субконто1,
	//Null КАК ВложенныйЗапрос.Субконто2,
	//Null КАК ВложенныйЗапрос.Субконто3,
	ВложенныйЗапрос.Валюта,
	ВложенныйЗапрос.БУНачальныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.БУНачальныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.БУКонечныйРазвернутыйОстатокДт,
	ВложенныйЗапрос.БУКонечныйРазвернутыйОстатокКт,
	ВложенныйЗапрос.Показатель,
	ВложенныйЗапрос.ЭтоРазвернутоеСальдо
ИЗ
	(ВЫБРАТЬ
		ОстаткиИОбороты.Счет КАК Счет,
		//Null КАК Субконто1,
		//Null КАК Субконто2,
		//Null КАК Субконто3,
		ОстаткиИОбороты.Валюта КАК Валюта,
		ОстаткиИОбороты.СуммаНачальныйРазвернутыйОстатокДт КАК БУНачальныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаНачальныйРазвернутыйОстатокКт КАК БУНачальныйРазвернутыйОстатокКт,
		ОстаткиИОбороты.СуммаКонечныйРазвернутыйОстатокДт КАК БУКонечныйРазвернутыйОстатокДт,
		ОстаткиИОбороты.СуммаКонечныйРазвернутыйОстатокКт КАК БУКонечныйРазвернутыйОстатокКт,
		1 КАК Показатель,
		ИСТИНА КАК ЭтоРазвернутоеСальдо
	ИЗ
		РегистрБухгалтерии.Хозрасчетный.ОстаткиИОбороты(
				{(&amp;НачалоПериода)},
				{(&amp;КонецПериода)},
				,
				, //Начало УсловиеСчета РазвернутоеСальдо
				Счет В ИЕРАРХИИ (&amp;СчетРазвернутоеСальдо)
					И НЕ Счет В (&amp;СчетаИсключенныеИзЗапросаПоСчетамРазвернутое)
					И (НЕ Счет.Забалансовый
						ИЛИ &amp;ВыводитьЗабалансовыеСчета)
				  //Конец УсловиеСчета РазвернутоеСальдо
				, //Начало Субконто РазвернутоеСальдо
				&amp;СубконтоРазвернутый
				  //Конец Субконто РазвернутоеСальдо
				,
				{(Организация).* КАК Организация}) КАК ОстаткиИОбороты) КАК ВложенныйЗапрос
	//Конец ЗапросПоСубконто РазвернутоеСальдо</query>
